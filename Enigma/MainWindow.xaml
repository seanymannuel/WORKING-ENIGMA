<Window x:Class="Enigma.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Enigma"
        mc:Ignorable="d"
         Title="Enigma Machine" Height="900" Width="1290" Background="#64CCC5">
    <Grid>
        <StackPanel Margin="0,0,0,556">
        <Menu Background="#279EFF">
            <MenuItem Header="File">
                <MenuItem 
        x:Name="MenuOpen" 
        Header="Open"
        Click="MenuOpen_Click"/>
                <Separator/>
                <MenuItem 
        x:Name="MenuClose"
        Header="Close"/>
            </MenuItem>

            <!--<MenuItem Header="Settings">
                <MenuItem x:Name="rSettings" Header="Rotor Settings"/>
            </MenuItem>-->
        </Menu>
    
    </StackPanel>
       
        <StatusBar Background="#279EFF"
VerticalAlignment="Bottom">
            <StatusBar VerticalAlignment="Bottom" Background="#279EFF" >
                <StackPanel Orientation="Horizontal">
                    <TextBlock x:Name="FilePathTextBlock" Text="File Path: " Margin="0,0,20,0"/>
                    <TextBlock x:Name="ringC" Text="Ring Count " Margin="0,0,20,0"/>
                    <TextBlock x:Name="rCount" Text="Character Count per Ring: " />
                </StackPanel>
            </StatusBar>
        </StatusBar>
